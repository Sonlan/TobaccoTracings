
.block_overlay {position:absolute;top:0%;left:0%;width:100%;height:100%;z-index:100;filter:alpha(opacity=80);}
.div_content {position:absolute;top:50px;left:20px;width:100%;height:500px;z-index:101;/*overflow:auto;*/background-color:#e6e2e7;border:2px solid #009a44;}
.modifyCloseImage{width:100%;height:28px;}
.modifyImageStyle{width:28px;height:28px;line-height:28px;float:right;cursor:pointer;}
.tip{font-size:12px;color:red;display:none}
.all{margin:50px auto;width:50%;}
.modifyDiv{margin-top:20px;margin-left:180px;}
.modifyLabel{float:left;width:80px;text-align:right;height:30px;line-height:30px;}
.modifyInput{width:254px;height:26px;margin-left:12px;border-radius:2.5px;border:1px solid #dbdbdb;}
.modifyInput:focus{border:1px solid #009a44;}
.modifyReset{margin-top:22px;margin-left:290px;float:left;width:100px;height:30px;line-height:30px;text-align:center;color:#00BFFF;cursor:pointer;/*background-image:url(../images/8.png);*/}
.modifySubmit{margin-top:22px;margin-left:20px;float:left;width:100px;height:30px;line-height:30px;text-align:center;color:#00BFFF;cursor:pointer;/*background-image:url(../images/8.png);*/}
.modifyReset:hover{color:#FFA500;}
.modifySubmit:hover{color:#FFA500;}